<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.survController">
    <center>
        <TableView fx:id="tableSurveillant">
            <columns>
                <TableColumn fx:id="infirmierNumCol" prefWidth="120" text="Infirmier Numéro" />
                <TableColumn fx:id="dateDebutCol" prefWidth="120" text="Date de début" />
                <TableColumn fx:id="heureFinCol" prefWidth="120" text="Heure de fin" />
            </columns>
        </TableView>
    </center>
    <bottom>
        <Button onAction="#getAjoutsurvView" text="Ajouter Surveillant" />
    </bottom>
    <padding>
        <Insets bottom="10" left="10" right="10" top="10" />
    </padding>
</BorderPane>
